{
  "name": "nleak",
  "description": "Automatical memory detection and diagnosis tool for NodeJS.",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "test:driver": "npx tsc && cd build/src/cli && node nleak.js run --config ../../test/example/config.js --out ../../test/example",
    "test:find-leaks": "rm -rf build && npx tsc && cd build/src/cli && node nleak.js run --config ../../test/example/config.js --out ../../test/example --guest-app-entry ../../test/example/guest_app.js",
    "test:leank-and-no-leak": "npm run test:leak &&  npm run test:no-leak",
    "test:leak": "env mocha -r ts-node/register 'test/leak_test.ts'",
    "test:no-leak": "env mocha -r ts-node/register 'test/no_leak_test.ts'"
    "test": "env mocha -r ts-node/register 'test/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nleak/nleak.git"
  },
  "keywords": [
    "NodeJS",
    "memory",
    "leak",
    "detection",
    "diagnosis"
  ],
  "author": "NLeak Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nleak/nleak/issues"
  },
  "homepage": "https://github.com/nleak/nleak#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chrome-remote-interface": "^0.31.4",
    "@types/esprima": "^4.0.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@types/node-fetch": "^2.6.2",
    "@types/progress": "^2.0.5",
    "@types/yargs": "^17.0.13",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "chrome-remote-interface": "^0.31.3",
    "error-stack-parser": "^2.1.4",
    "esprima": "^4.0.1",
    "node-fetch": "^2.6.7",
    "progress": "^2.0.3",
    "yargs": "^17.6.0"
  }
}
